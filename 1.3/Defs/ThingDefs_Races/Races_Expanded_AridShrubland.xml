			<wildBiomes>
				<AridShrubland>0.8</AridShrubland>
				<Desert>0.4</Desert>
				<ExtremeDesert>0.1</ExtremeDesert>
			</wildBiomes>
			<gestationPeriodDays>12</gestationPeriodDays>
			<litterSizeCurve>
				<points>
					<li>(0.5, 0)</li>
					<li>(1, 1)</li>
					<li>(2, 1.7)</li>
					<li>(2.5, 0.7)</li>
					<li>(3, 0)</li>
