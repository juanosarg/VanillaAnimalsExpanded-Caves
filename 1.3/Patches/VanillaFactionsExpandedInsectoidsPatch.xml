<?xml version="1.0" encoding="utf-8" ?>

<Patch>


	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Insectoids</li>
		</mods>

		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>

				<li Class="PatchOperationAdd">

					<xpath>/Defs/ThingDef[defName = "VAECaves_CaveCrawler"]</xpath>
					<value>

					<butcherProducts>
						<VFEI_Chitin>40</VFEI_Chitin>
					</butcherProducts>
				



					</value>
				</li>
				
				<li Class="PatchOperationAdd">

					<xpath>/Defs/ThingDef[defName = "VAECaves_InsectoidHulk"]</xpath>
					<value>

					<butcherProducts>
						<VFEI_Chitin>60</VFEI_Chitin>
					</butcherProducts>
				



					</value>
				</li>
				
				<li Class="PatchOperationAdd">

					<xpath>/Defs/ThingDef[defName = "VAECaves_GiantSpider"]</xpath>
					<value>

					<butcherProducts>
						<VFEI_Chitin>60</VFEI_Chitin>
					</butcherProducts>
				



					</value>
				</li>
				<li Class="PatchOperationAdd">

					<xpath>/Defs/ThingDef[defName = "VAECaves_AncientGiantSpider"]</xpath>
					<value>

					<butcherProducts>
						<VFEI_Chitin>200</VFEI_Chitin>
					</butcherProducts>
				



					</value>
				</li>
				
				
				
				



			</operations>
		</match>
	</Operation>


</Patch>

